{
    "name": "maymeow/blog-standalone-testing",
    "description": "Blog plugin for CakePHP - example for standalone testing",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Blog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blog\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
            "App\\": "tests/test_app/src/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
